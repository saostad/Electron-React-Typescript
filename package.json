{
  "name": "create-electron-ts-react",
  "productName": "electron-ts-react",
  "version": "3.1.16",
  "description": "Electron Typescript React Boilerplate / Starter",
  "repository": {
    "type": "git",
    "url": "https://github.com/saostad/Electron-React-Typescript.git"
  },
  "bin": "./bin/setup.js",
  "main": ".webpack/main",
  "scripts": {
    "tag": "node ./config/tag.js",
    "auto-changelog": "auto-changelog --hide-credit --breaking-pattern \"breaking:\"",
    "gen-changelog": "npm run auto-changelog && git add CHANGELOG.md && git commit -m \"doc: change log\"",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publishOnly": "electron-forge publish",
    "postpublish": "npm-run-all -s tag gen-changelog"
  },
  "keywords": [
    "electron",
    "typescript",
    "react",
    "starter",
    "boilerplate"
  ],
  "author": "Saeid Ostad",
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "electron-ts-react"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        {
          "name": "@electron-forge/plugin-webpack",
          "config": {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./public/index.html",
                  "js": "./src/renderer/renderer.tsx",
                  "name": "main_window"
                }
              ]
            }
          }
        }
      ]
    }
  },
  "dependencies": {
    "@hot-loader/react-dom": "^17.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.2",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/runtime": "^7.20.6",
    "@electron-forge/cli": "^6.0.4",
    "@electron-forge/maker-deb": "^6.0.4",
    "@electron-forge/maker-rpm": "^6.0.4",
    "@electron-forge/maker-squirrel": "^6.0.4",
    "@electron-forge/maker-zip": "^6.0.4",
    "@electron-forge/plugin-webpack": "^6.0.4",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@types/node": "^16.18.4",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "@typescript-eslint/parser": "^5.45.1",
    "auto-changelog": "^2.4.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^9.1.0",
    "css-loader": "^6.7.2",
    "electron": "^21.3.3",
    "electron-devtools-installer": "^3.2.0",
    "electron-squirrel-startup": "^1.0.0",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "file-loader": "^6.2.0",
    "node-loader": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "react-hot-loader": "^4.13.1",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.3",
    "url-loader": "^4.1.1"
  }
}
